package llists;

public class Node {
    String element;
    Node next;
    
    public Node() {
        element = "";
        next = null;
    }
    
    public void setElement(String e){ element = e;}
    public void setNext(Node n){next = n;}
    
    public String getElement() {return element;}
    public Node getNext() {return next;}
    
    public void addNode(){
        setNext(new Node());
    }



    
}
